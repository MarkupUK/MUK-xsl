            <subtitle font-family="{$title.fontset}" text-align="center"/>
            <!--<corpauthor/>
            <authorgroup/>
            <author/>
            <othercredit/>-->
            <releaseinfo/>
            <copyright/>
            <legalnotice/>
            <pubdate/>
            <revision/>
            <revhistory/>
            <abstract/>
            <itermset/>
        </t:titlepage-content>
        <t:titlepage-content t:side="verso"/>
        <t:titlepage-separator>
  </t:titlepage-separator>
        <t:titlepage-before t:side="recto"/>
        <t:titlepage-before t:side="verso"/>
